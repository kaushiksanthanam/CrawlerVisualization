<UserControl xmlns:graphx="http://schemas.panthernet.ru/graphx/"  x:Class="CrawlerVisualization.VisualizationWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CrawlerVisualization"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="root" Loaded="root_Loaded_1">
    <Grid>
        <graphx:ZoomControl x:Name="gg_zoomctrl">
            <local:GraphAreaExample x:Name="gg_Area"/>
        </graphx:ZoomControl>
    </Grid>
</UserControl>
